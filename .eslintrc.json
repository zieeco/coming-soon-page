{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:import/errors",
        "plugin:import/warnings",
        "next/core-web-vitals"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jest": true
    },
    "parser": "@typescript-eslint/parser",
        "parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
    },
    "root": true,
    "plugins": ["@typescript-eslint"],
    "ignorePatterns": [
    "dist/",
    "build/"
    ]
}